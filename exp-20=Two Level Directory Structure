#include <stdio.h>
#include <string.h>
int main() {
    char user[5][20], file[5][5][20];
    int u, f, i, j;
    printf("Enter number of users: ");
    scanf("%d", &u);
    for (i = 0; i < u; i++) {
        printf("Enter user name: ");
        scanf("%s", user[i]);
        printf("Enter number of files for %s: ", user[i]);
        scanf("%d", &f);
        for (j = 0; j < f; j++) {
            scanf("%s", file[i][j]);
        }
    }

    printf("\nDirectory Structure:\n");
    for (i = 0; i < u; i++) {
        printf("User: %s\n", user[i]);
        for (j = 0; j < 5; j++) {
            if (strlen(file[i][j]) > 0)
                printf("  %s\n", file[i][j]);
        }
    }
    return 0;
}
