#include <stdio.h>

int main() {
    int pages[8] = {7, 0, 1, 2, 0, 3, 0, 4};
    int frame[3] = {-1, -1, -1};
    int faults = 0, index = 0;

    for (int i = 0; i < 8; i++) {
        int found = 0;
        for (int j = 0; j < 3; j++)
            if (frame[j] == pages[i]) found = 1;

        if (!found) {
            frame[index] = pages[i];
            index = (index + 1) % 3;
            faults++;
        }
    }
    printf("FIFO Page Faults = %d\n", faults);
    return 0;
}
