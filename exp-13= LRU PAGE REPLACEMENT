#include <stdio.h>

int findLRU(int time[], int n) {
    int min = time[0], pos = 0;
    for (int i = 1; i < n; i++) {
        if (time[i] < min) {
            min = time[i];
            pos = i;
        }
    }
    return pos;
}

int main() {
    int pages[8] = {7, 0, 1, 2, 0, 3, 0, 4};
    int frame[3], time[3];
    int faults = 0, counter = 0;

    for (int i = 0; i < 3; i++) frame[i] = -1;

    for (int i = 0; i < 8; i++) {
        int found = 0;
        for (int j = 0; j < 3; j++) {
            if (frame[j] == pages[i]) {
                counter++;
                time[j] = counter;
                found = 1;
   }
        }
        if (!found) {
            int pos = findLRU(time, 3);
            counter++;
            frame[pos] = pages[i];
            time[pos] = counter;
            faults++;
        }
    }
    printf("LRU Page Faults = %d\n", faults);
    return 0;
}

